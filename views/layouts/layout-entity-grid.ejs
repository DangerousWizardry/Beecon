<div class="entity" @click="clickEntity(entity)" v-for="entity in entities">
  <img src="/images/medic.png" alt="">
  <div class="entity-info">
    <span class="display-name">{{entity.entityDisplayName}}</span>
    <span class="last-received" v-if="typeof entity.positions[0] != 'undefined'">Derni√®re reception {{ renderDurationName(entity.positions[0].timestamp) }}</span>
  </div>
  <i class="fa fa-2x fa-signal" v-if="typeof entity.positions[0] != 'undefined'" :class="renderSignal(entity.positions[0].timestamp)"></i>
</div>